package com.day1.session2.ex3;
import java.util.*;
public class AppleApp {
	
	public static List<Apple> getAllApplesOnPredicate(List<Apple> apples){
		List<Apple> list=new ArrayList<>();
		for(Apple temp: apples) {
			if(temp.getColor().equals("green"))
				list.add(temp);
		}
		return list;
		
	}
	
	
	
}
